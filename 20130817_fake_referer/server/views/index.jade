extends layout

block content
  h1= title
  script(src='http://code.jquery.com/jquery-1.9.1.min.js')
  script
    function getUserList(){
      $.ajax({
        url: 'users'
      }).done(function(data) {
        $(user).empty().text(data);
      });
    }

  input(type="button", value="Get user list", onclick="getUserList()")
  #user